      "id": "894fc61d-ca28-478f-a870-7318bd347a72",
      "name": "AI Automation RSS",
      "type": "n8n-nodes-base.rssFeedRead",
      "typeVersion": 1.2,
      "position": [
        240,
        544
      ]
    },
    {
      "parameters": {
        "jsCode": "// Keywords for AI automation, business, money-making\nconst keywords = [\n  'automation', 'automate', 'workflow', 'business', 'enterprise',\n  'startup', 'company', 'money', 'revenue', 'profit', 'income',\n  'monetize', 'ai agent', 'chatgpt', 'openai', 'claude', 'gemini',\n  'llm', 'gpt', 'machine learning', 'saas', 'tool', 'platform'\n];\n\nconst now = new Date();\nconst maxAgeDays = 5;\nconst results = [];\nconst seenTitles = new Set();\n\nfor (const item of $input.all()) {\n  const title = (item.json.title || '').trim();\n  const titleLower = title.toLowerCase();\n  \n  if (seenTitles.has(titleLower)) continue;\n  seenTitles.add(titleLower);\n  \n  const desc = (item.json.description || item.json.contentSnippet || '').toLowerCase();\n  const content = titleLower + ' ' + desc;\n  \n  const pubDate = new Date(item.json.pubDate || item.json.isoDate || now);\n  const ageInDays = (now - pubDate) / (1000 * 60 * 60 * 24);\n  if (ageInDays > maxAgeDays) continue;\n  \n  let score = 0;\n  for (const kw of keywords) {\n    if (content.includes(kw)) score++;\n  }\n  if (score === 0) continue;\n  \n  results.push({\n    title: title,\n    link: item.json.link,\n    description: (item.json.contentSnippet || item.json.description || '').substring(0, 300),\n    pubDate: item.json.pubDate,\n    score: score,\n    ageInDays: Math.round(ageInDays * 10) / 10\n  });\n}\n\nresults.sort((a, b) => b.score - a.score || a.ageInDays - b.ageInDays);\nconst top5 = results.slice(0, 5);\n\nreturn [{\n  json: {\n    totalFound: results.length,\n    articles: top5\n  }\n}];"
      },
