      "parameters": {
        "jsCode": "// Combine all content and media URLs\nconst content = $('Format Content').first().json;\nconst linkedinImg = $('Retrieve LinkedIn Image').first().json;\nconst twitterImg = $('Retrieve Twitter Image').first().json;\nconst instagramVid = $('Retrieve Instagram Video').first().json;\nconst youtubeVid = $('Retrieve YouTube Video').first().json;\n\nconst linkedinImageUrl = linkedinImg.images?.[0]?.url || linkedinImg.url || '';\nconst twitterImageUrl = twitterImg.images?.[0]?.url || twitterImg.url || '';\nconst instagramVideoUrl = instagramVid.video_url || instagramVid.url || '';\nconst youtubeVideoUrl = youtubeVid.video_url || youtubeVid.url || '';\n\nreturn [{\n  json: {\n    ...content,\n    linkedin_image_url: linkedinImageUrl,\n    twitter_image_url: twitterImageUrl,\n    instagram_video_url: instagramVideoUrl,\n    youtube_video_url: youtubeVideoUrl,\n    status: 'ready_for_approval',\n    created_at: new Date().toISOString()\n  }\n}];"
