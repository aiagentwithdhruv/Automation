    {
      "parameters": {
        "promptType": "define",
        "text": "=Resume Text:\n{{ $('Extract Resume Text').item.json.text }}\n\nJob Requirements:\n{{ $('Fetch Job Requirements').item.json.requirements }}\n\nPlease evaluate this candidate.",
        "hasOutputParser": true,
        "options": {
          "systemMessage": "You are an expert resume screening agent. Analyze the candidate's resume against the job requirements and provide a structured evaluation.\n\nEvaluate the candidate on:\n1. Skills match (0-100)\n2. Experience match (0-100)\n3. Education match (0-100)\n4. Overall fit (0-100)\n\nProvide:\n- recommendation: \"shortlist\", \"maybe\", or \"reject\"\n- overall_score: numeric score\n- skills_match, experience_match, education_match: numeric scores\n- key_strengths: array of strengths\n- concerns: array of concerns\n- summary: brief evaluation summary\n\nBe objective and focus on job-relevant qualifications."
        }
      },
      "id": "0a466da2-2a4b-4270-bb70-e793217d071b",
      "name": "AI Screening Agent",
      "type": "@n8n/n8n-nodes-langchain.agent",
      "typeVersion": 3.1,
      "position": [
        -784,
        96
      ]
    },
